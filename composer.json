{
    "name": "Max",
    "description": "Max(base on slim)",
    "require": {
        "slim/slim": "2.6.2",
        "slim/extras": "2.0.3",
        "slim/views":"0.1.3",
        "twig/twig": "1.20.0"
    },
    "autoload": {
        "psr-0": {
            "Max\\": "lib/",
            "Controller\\": "./",
            "Models\\": "./"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.phpcomposer.com"
        },
        {
            "packagist": false
        }
    ]
}
